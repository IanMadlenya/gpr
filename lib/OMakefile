LIB_MLNAMES[] =
  interfaces
  utils
  kernel_impl
  fitc_impl
  fitc
  spgp_impl
  spgp
  experimental

section
  OCAMLCFLAGS += -for-pack Gpr
  OCAMLOPTFLAGS += -for-pack Gpr
  $(addsuffixes $(OCAML_SUFFIXES), $(LIB_MLNAMES)):

OCamlPackage(gpr, $(LIB_MLNAMES))
OCamlLibrary(gpr, gpr)

OCamlProgram(spgp_impl, utils spgp_impl)

InstClean()
